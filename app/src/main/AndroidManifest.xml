<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.student.omclasses">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.student.omclasses.PlayVideo"></activity>
        <activity android:name="com.student.omclasses.Profile" />
        <activity
            android:name="com.student.omclasses.Home"
            android:screenOrientation="locked" />
        <activity
            android:name="com.student.omclasses.ResetPassword"
            android:screenOrientation="locked" />
        <activity
            android:name="com.student.omclasses.Login"
            android:screenOrientation="locked" />
        <activity
            android:name="com.student.omclasses.ViewList"
            android:screenOrientation="locked" />
        <activity
            android:name="com.student.omclasses.MainActivity"
            android:screenOrientation="locked">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>